<div class="openaside"><a class="navbutton" href="#" title="<%= __('showsidebar') %>"></a></div>
<% if( table&&(item.toc !== false) && theme.toc.aside){ %>
  <div id="toc" class="toc-aside">
  <strong class="toc-title"><%= __('contents') %></strong>
 <% if(item.list_number == false) {%>
 <%- toc(item.content,{list_number:false}) %>
 <% }else{ %>
 <%- toc(item.content) %>
 <% } %>
  </div>
<% } %>
<div id="asidepart">
<div class="closeaside"><a class="closebutton" href="#" title="<%= __('hidesidebar') %>"></a></div>
<aside class="clearfix">
	<% if (site.data.annexes){ 
	for (key in site.data.annexes){
	var annexe = site.data.annexes[key] %>
	<div class="tagslist">
	<p class="asidetitle"><%= annexe.title %></p>
		<ul class="clearfix">
		<% site.tags.sort('posts.length','desc').limit(20).each(function(item){ %>
			<% if(item.posts.length){ %>
				<li><a href="<%- config.root %><%- item.path %>" title="<%= item.name %>"><%= item.name %><sup><%= item.posts.length %></sup></a></li>
			<% } %>
		<% }); %>
		</ul>
	</div>
	<% } } %>
</aside>
</div>